{
  "private": true,
  "name": "toolingjs",
  "description": "Monorepo for Toolingjs",
  "packageManager": "npm@9.5.1",
  "workspaces": [
    "libs/*"
  ],
  "scripts": {
    "build": "turbo run build",
    "dev": "turbo run dev",
    "lint": "turbo run lint",
    "prepare": "husky install"
  },
  "devDependencies": {
    "@changesets/cli": "^2.26.2",
    "husky": "^8.0.3",
    "turbo": "^1.10.12"
  },
  "prettier": "@toolingjs/prettier",
  "commitlint": {
    "extends": [
      "@toolingjs/commitlint"
    ]
  }
}